{
  "public_endpoints": [
    "/docs",
    "/openapi.json",
    "/favicon.ico",
    "/",
    "/api/authentication/login",
    "/api/authentication/register"
  ],
  "all_permissions": [
    "read_users",
    "create_users",
    "read_books",
    "update_books",
    "delete_books"
  ],
  "endpoints": {
    "/users": {
      "GET": [
        "read_users"
      ],
      "POST": [
        "create_users"
      ]
    },
    "/books/{book_id}": {
      "GET": [
        "read_books"
      ],
      "PUT": [
        "update_books"
      ],
      "DELETE": [
        "delete_books"
      ]
    }
  }
}